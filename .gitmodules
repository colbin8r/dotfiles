[submodule "zsh/pure"]
	path = zsh/pure
	url = https://github.com/sindresorhus/pure
[submodule "zsh/.oh-my-zsh"]
	path = zsh/.oh-my-zsh
	url = https://github.com/robbyrussell/oh-my-zsh.git
[submodule "zsh/.oh-my-zsh/custom/plugins/zsh-better-npm-completion"]
	path = zsh/.oh-my-zsh/custom/plugins/zsh-better-npm-completion
	url = https://github.com/lukechilds/zsh-better-npm-completion
	branch = master
[submodule "zsh/zsh_custom/powerlevel9k"]
	path = zsh/zsh_custom/powerlevel9k
	url = https://github.com/bhilburn/powerlevel9k
[submodule "zsh/zsh_custom/plugins/zsh-syntax-highlighting"]
	path = zsh/zsh_custom/plugins/zsh-syntax-highlighting
	url = https://github.com/zsh-users/zsh-syntax-highlighting.git
